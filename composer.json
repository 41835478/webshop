{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
		"laravel/framework": "5.3.*",
        "laravelcollective/html": "5.3",
        "guzzlehttp/guzzle": "6.0.*",
		"barryvdh/laravel-debugbar": "^2.0",
        "laravel/cashier": "~7.0",
		"intervention/image": "^2.2",
        "gloudemans/shoppingcart": "~1.3",
        "mathiasverraes/money": "dev-master",
        "barryvdh/laravel-dompdf": "0.7.x@dev",
        "maatwebsite/excel": "~2.0.0",
        "venturecraft/revisionable": "1.*",
		"laravel/socialite": "^3.0@dev",
        "simplesoftwareio/simple-qrcode": "1.3.2",
        "torann/registry": "0.2.*@dev",
        "baum/baum": "~1.1",
        "inlinestyle/inlinestyle": "1.*",
        "clegginabox/pdf-merger": "dev-master",
		"designpond/newsletter" : "dev-master",
		"vinkla/alert": "^1.2",
		"spatie/laravel-backup": "^4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.0",
		"phpspec/phpspec": "~2.1",
		"benconstable/phpspec-laravel": "~2.0",
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "1.6.0",
        "symfony/dom-crawler":"~3.0",
        "symfony/css-selector" : "~3.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"Box\\Spout\\": "vendor/box/spout/src/Spout/"
		},
		"files": [
			"tests/factories/ObjectFactory.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
