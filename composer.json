{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
		"laravel/framework": "5.2.*",
        "laravelcollective/html": "5.2",
        "guzzlehttp/guzzle": "5.1.0",
        "league/oauth2-server": "4.1.4",
        "lucadegasperi/oauth2-server-laravel": "dev-master",
		"barryvdh/laravel-debugbar": "^2.0",
        "laravel/cashier": "~5.0",
		"intervention/image": "^2.2",
        "gloudemans/shoppingcart": "~1.3",
        "mathiasverraes/money": "dev-master",
        "barryvdh/laravel-dompdf": "0.6.*",
        "maatwebsite/excel": "~2.0.0",
        "omnipay/stripe": "~2.0",
        "venturecraft/revisionable": "1.*",
		"laravel/socialite": "^3.0@dev",
        "simplesoftwareio/simple-qrcode": "1.3.2",
        "torann/registry": "0.2.*@dev",
        "baum/baum": "~1.1",
        "inlinestyle/inlinestyle": "1.*",
        "clegginabox/pdf-merger": "dev-master",
		"pusher/pusher-php-server": "~2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "1.4.0",
        "symfony/dom-crawler":"~3.0",
        "symfony/css-selector" : "~3.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
